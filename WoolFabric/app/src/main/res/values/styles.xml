<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="newtheme">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
    </style>

    <style name="dialog" parent="Widget.AppCompat.ButtonBar.AlertDialog">
        <item name="colorPrimary">#F0D56188</item>
        <item name="colorPrimaryVariant">#F0D56188</item>
        <item name="android:textColor">#F0D56188</item>
    </style>
<style name="themenew" parent="Theme.MaterialComponents.Light.DarkActionBar">
<item name="colorPrimary">@color/background</item>
    <item name="colorPrimaryVariant">@color/background</item>
    <item name="colorAccent">@color/background</item>
</style>
    <style name="tabtextapperance">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="farmer" parent="Theme.AppCompat.DayNight.NoActionBar">

        <item name="colorPrimary">#FF5722</item>
        <item name="colorPrimaryVariant">#FF5722</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="fontFamily">@font/robotomedium</item>
        <item name="font">@font/robotomedium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:font" tools:ignore="NewApi">@font/robotomedium</item>
        <item name="android:fontFamily">@font/robotomedium</item>

        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#F0D56188</item>
        <!-- Status bar color. -->
        <item name="android:navigationBarColor">@color/background</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">true</item>
        <item name="android:statusBarColor" >#6A6C6C</item>

    </style>

    <style name="searchtextapperance">

        <item name="hintTextColor">@color/white</item>
        <item name="colorSecondary">@color/white</item>
        <item name="colorSecondaryVariant">@color/white</item>
<item name="editTextColor">@color/white</item>
        <item name="android:editTextColor">@color/white</item>
        <item name="tint">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>